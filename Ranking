 lst = {'Romance': s1,'Mindset': s2,'Fitness': s3,'Career': s4,'Free Time': s5,'Karma': s6, 'Friends': s7, 'Self care': s8, 'Family': s9, 'Finance': s10}
    def sort_dict_by_value(d, reverse = False):
        return dict(sorted(d.items(), key = lambda x: x[1], reverse = reverse))
    print("\nCategories from lowest to highest ranking:")
    print(sort_dict_by_value(lst))
